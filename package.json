{
  "name": "blockid-network",
  "version": "0.0.1",
  "description": "hyperleger composer - block ID business network",
  "scripts": {
    "test": "mocha --recursive",
    "update-bn": "composer network update -c admin@blockid-network -a blockid-network@0.0.1.bna",
    "pack-bn": "composer archive create -t dir -n .",
    "start-bn-api": "composer-rest-server -c admin@blockid-network -n never -w true",
    "update-api": "npm run pack-bn && npm run update-bn && npm run start-bn-api"
  },
  "author": "Justin Rao",
  "email": "justin.rao@rbc.com",
  "license": "Apache-2.0",
  "devDependencies": {
    "composer-admin": "latest",
    "composer-client": "latest",
    "composer-common": "latest",
    "composer-connector-embedded": "latest",
    "chai": "latest",
    "eslint": "latest",
    "istanbul": "latest",
    "mkdirp": "latest",
    "mocha": "latest"
  }
}
